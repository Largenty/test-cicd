{
    "ci": {
      "collect": {
        "staticDistDir": "./dist",
        "numberOfRuns": 3,
        "settings": {
          "preset": "desktop"
        }
      },
      "assert": {
        "assertions": {
          "categories:performance": ["errore", {"minScore": 0.9}],
          "categories:accessibility": ["errore", {"minScore": 0.99}],
          "categories:best-practices": ["errore", {"minScore": 0.99}],
          "categories:seo": ["errore", {"minScore": 0.7}],
          "color-contrast": "off",
          "meta-description": "off",
          "network-dependency-tree-insight": "off",
          "unsized-images": "off",
          "unused-javascript": "off",
          "render-blocking-insight": "off"
        }
      },
      "upload": {
        "target": "temporary-public-storage"
      }
    }
  }